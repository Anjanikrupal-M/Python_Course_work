#1.slary
salary = float(input())
if salary <= 250000:
    print("No Tax")
elif 250000 < salary <= 500000:
    print(salary*0.05)
elif 500000 < salary <=1000000:
    print(salary*0.2)
else:
    print(salary*0.5)    



